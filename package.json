{
  "name": "test",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:verbose": "ng serve --verbose",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "fix:prettier": "prettier --write .",
    "fix:lint:strict": "eslint -c ./eslint.strict.config.js \"**/*.{ts,html}\" --fix",
    "fix:all": "npm run fix:prettier && npm run fix:lint:strict",
    "check:all": "npm run build && npm run test && npm run lint",
    "build:gh-pages": "ng build --configuration production --base-href /new-features-try/",
    "serve:gh-pages": "ng build --configuration production --base-href /new-features-try/ && npx http-server dist/test -p 4201",
    "prepare": "husky",
    "lint:strict": "eslint -c ./eslint.strict.config.js \"**/*.{ts,html}\"",
    "precommit": "lint-staged"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/material": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "primeflex": "^3.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.1",
    "@angular/cli": "~19.0.1",
    "@angular/compiler-cli": "^19.0.0",
    "@types/jasmine": "~4.3.0",
    "angular-eslint": "19.0.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-sonarjs": "^3.0.4",
    "eslint-plugin-unicorn": "^60.0.0",
    "husky": "^9.1.7",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.3",
    "typescript-eslint": "8.18.0"
  },
  "lint-staged": {
    "*.{ts,html}": [
      "prettier --write",
      "eslint -c ./eslint.strict.config.js --fix"
    ],
    "*.{scss,css,md,json,yml,yaml}": [
      "prettier --write"
    ]
  }
}
